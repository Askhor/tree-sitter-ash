{
    "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
    "grammars": [
        {
            "name": "ash",
            "camelcase": "Ash",
            "title": "Ash",
            "scope": "source.ash",
            "file-types": [
                "ash"
            ],
            "injection-regex": "^ash$",
            "class-name": "TreeSitterAsh",
            "highlights": "queries/highlights.scm"
        }
    ],
    "metadata": {
        "version": "0.1.0",
        "license": "MIT",
        "description": "The parser for Ash, a parser generator",
        "authors": [
            {
                "name": "J. G\u00fcnthner",
                "email": "j@guenthner.xyz",
                "url": "https://guenthner.xyz/"
            }
        ],
        "links": {
            "repository": "https://github.com/Askhor/tree-sitter-ash"
        }
    },
    "bindings": {
        "c": true,
        "go": true,
        "node": true,
        "python": true,
        "rust": true,
        "swift": true,
        "zig": false
    }
}