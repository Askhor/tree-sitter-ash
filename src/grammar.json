{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "ash",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "identifier": {
      "type": "PATTERN",
      "value": "\\w+"
    },
    "regular_expr": {
      "type": "PATTERN",
      "value": "\\|.*"
    },
    "regex_ref": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "comment": {
      "type": "PATTERN",
      "value": "\\/{2}.*"
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "token_definition"
        },
        {
          "type": "SYMBOL",
          "name": "node_definition"
        },
        {
          "type": "SYMBOL",
          "name": "abstract_node_definition"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "_node_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "node"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "is"
                },
                {
                  "type": "SYMBOL",
                  "name": "identifier"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "token_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "token"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "regular_expr"
        }
      ]
    },
    "node_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_node_declaration"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_field"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "abstract_node_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "abstract"
        },
        {
          "type": "SYMBOL",
          "name": "_node_declaration"
        }
      ]
    },
    "_field": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "node_field"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "node_field": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "STRING",
                      "value": "optional"
                    },
                    {
                      "type": "SYMBOL",
                      "name": "_repeat_modifier"
                    }
                  ]
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "SYMBOL",
              "name": "identifier"
            },
            {
              "type": "STRING",
              "value": "is"
            },
            {
              "type": "SYMBOL",
              "name": "identifier"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "regular_expr"
        },
        {
          "type": "SYMBOL",
          "name": "regex_ref"
        }
      ]
    },
    "_repeat_modifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "array"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "["
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "number"
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                },
                {
                  "type": "STRING",
                  "value": ","
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "number"
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                },
                {
                  "type": "STRING",
                  "value": "]"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}